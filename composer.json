{
    "name": "pdffiller/qless-php",
    "description": "PHP Bindings for qless",
    "keywords": [
        "qless",
        "queue",
        "jobs",
        "lua",
        "redis",
        "worker",
        "message",
        "messages"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Serghei Iakovlev",
            "email": "i.serghei@pdffiller.com"
        },
        {
            "name": "contatta",
            "email": "dev@contatta.com"
        }
    ],
    "require": {
        "php": ">=7.1 <8.0",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-redis": "*",
        "ext-sockets": "*",
        "infection/infection": "0.10.*",
        "phpstan/phpstan": "0.10.*",
        "psr/log": "^1.0",
        "ramsey/uuid": "3.8.*"
    },
    "conflict": {
        "contatta/qless-php": "*"
    },
    "require-dev": {
        "monolog/monolog": "^1.23",
        "phpunit/phpunit": ">=5.3 <8.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Qless\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Qless\\Demo\\": "demo/",
            "Qless\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "fix-lint": "phpcbf",
        "lint": "phpcs",
        "test": "phpunit --disallow-test-output",
        "bench": "phpunit --group performance --dont-report-useless-tests --no-coverage --no-logging",
        "analyse": "phpstan analyse -l max src"
    }
}
