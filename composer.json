{
    "name": "pdffiller/qless-php",
    "description": "PHP Bindings for qless",
    "keywords": [
        "qless",
        "queue",
        "jobs",
        "lua",
        "redis",
        "worker",
        "message",
        "messages"
    ],
    "homepage": "https://pdffiller.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Serghei Iakovlev",
            "email": "i.serghei@pdffiller.com"
        },
        {
            "name": "contatta",
            "email": "dev@contatta.com"
        }
    ],
    "require": {
        "php": ">=7.1 <8.0",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-pcre": "*",
        "ext-posix": "*",
        "ext-sockets": "*",
        "monolog/monolog": "^1.23",
        "predis/predis": "1.1.*",
        "psr/log": "^1",
        "ramsey/uuid": "3.7.* || 3.8.*",
        "seld/signal-handler": "1.1.*"
    },
    "conflict": {
        "contatta/qless-php": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "0.10.*",
        "phpunit/phpunit": ">=7.5 <8.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "suggest": {
        "ext-proctitle": "Needed to support setting process title as an unprivileged user on macOS"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Qless\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qless\\Demo\\": "demo/",
            "Qless\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/pdffiller/qless-php/issues"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
